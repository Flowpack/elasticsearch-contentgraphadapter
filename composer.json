{
    "description": "The ElasticSearch adapter for the Neos Content Repository using the in-memory content graph",
    "type": "neos-package",
    "name": "flowpack/elasticsearch-contentgraphadapter",
    "require": {
        "neos/neos": "~5.0 || ~7.0 || dev-master",
        "flowpack/elasticsearch-contentrepositoryadaptor": "~7.0 || ~8.0",
        "neos/content-repository-inmemorygraph": "*"
    },
    "autoload": {
        "psr-4": {
            "Flowpack\\ElasticSearch\\ContentGraphAdapter\\": "Classes"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Flowpack.ElasticSearch.ContentGraphAdapter"
        }
    }
}
